{
  "name": "tcl-outline",
  "displayName": "Tcl Outline",
  "description": "Outline for Tcl in VS Code",
  "version": "0.1.3",
  "publisher": "sheng",
  "license": "MIT",
  "engines": {
    "vscode": "^1.4.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.tclOutlineUnsorted",
        "title": "tcl outline (not sorted)"
      },
      {
        "command": "extension.tclOutlineSorted",
        "title": "tcl outline (sorted)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "tslint": "^5.4.3",
    "typescript": "^2.4.1",
    "vscode": "^1.1.1",
    "@types/node": "^8.0.7"
  },
  "bugs": {
    "url": "https://github.com/raycarter/tcl-outline-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raycarter/tcl-outline-vscode"
  }
}
